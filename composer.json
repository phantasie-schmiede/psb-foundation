{
	"authors": [
		{
			"email": "dn@phantasie-schmiede.de",
			"name": "Daniel Ablass",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"PSB\\PsbFoundation\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PSB\\PsbFoundation\\Tests\\": "Tests/"
		}
	},
	"config": {
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"description": "Configuration framework for TYPO3 extension development",
	"extra": {
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "psb_foundation",
			"web-dir": ".Build/public"
		}
	},
	"keywords": [
		"configuration",
		"development",
		"extension",
		"typo3"
	],
	"license": "GPL-3.0-or-later",
	"name": "psb/psb-foundation",
	"require": {
		"ext-fileinfo": "*",
		"ext-simplexml": "*",
		"php": "^8.1",
		"typo3/cms-core": "^12"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"typo3/testing-framework": "^6.8"
	},
	"scripts": {
		"ci:tests:unit": ".Build/vendor/bin/phpunit -c .Build/vendor/typo3/testing-framework/Resources/Core/Build/UnitTests.xml Tests/Unit",
		"prepare-release": [
			"rm -rf .Build",
			"rm -rf .github",
			"rm -rf Tests",
			"rm .editorconfig",
			"rm .gitattributes",
			"rm .gitignore"
		]
	},
	"type": "typo3-cms-extension"
}
